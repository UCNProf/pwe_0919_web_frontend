doctype html
html
  head
    title= title
    link(rel='stylesheet', href='/stylesheets/style.css')

    block head

    -var menu = {'home': '/', 'users': '/users', 'notes': '/notes', 'about': '/about'};
  body
    nav
      ul
        each val, key in menu
          li
            if id == key
              a.current(href=val)= key
            else
              a(href=val)= key


    main
      block content
